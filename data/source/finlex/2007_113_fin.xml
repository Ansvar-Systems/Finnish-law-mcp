        <preface>
            <p>
                <docNumber>113/2007</docNumber>
                <docTitle>Laki kiinteän omaisuuden ja erityisten oikeuksien lunastuksesta annetun lain 9 §:n muuttamisesta</docTitle>
            </p>
        </preface>
        <preamble>
