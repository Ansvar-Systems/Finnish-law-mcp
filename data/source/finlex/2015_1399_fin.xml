        <preface>
            <p>
                <docNumber>1399/2015</docNumber>
                <docTitle>Laki kuolleeksi julistamisesta annetun lain 14 §:n muuttamisesta </docTitle>
            </p>
        </preface>
        <preamble>
            <formula name="enactingClause">
                <p>Eduskunnan päätöksen mukaisesti </p>
                <blockContainer>
