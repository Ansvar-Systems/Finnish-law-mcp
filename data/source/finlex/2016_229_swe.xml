        <preface>
            <p>
                <docNumber>229/2016</docNumber>
                <docTitle>Lag om ändring av 21 § i viltskadelagen</docTitle>
            </p>
        </preface>
        <preamble>
            <formula name="enactingClause">
                <p>I enlighet med riksdagens beslut</p>
                <blockContainer>
                    <block name="substitutions">
                        <i>ändras</i>
                         i viltskadelagen (
                        <affectedDocument href="/akn/fi/act/statute/2009/105">105/2009</affectedDocument>
                        ) 21 § 1 mom. som följer:
                    </block>
