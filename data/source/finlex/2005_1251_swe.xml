        <preface>
            <p>
                <docNumber>1251/2005</docNumber>
                <docTitle>Lag om ändring av territorialövervakningslagen</docTitle>
            </p>
        </preface>
        <preamble>
            <formula name="enactingClause">
                <p>I enlighet med riksdagens beslut</p>
                <blockContainer>
                    <block name="substitutions">
