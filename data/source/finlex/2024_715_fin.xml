        <preface>
            <p>
                <docNumber>715/2024</docNumber>
                <docTitle>Laki puolustusvoimista annetun lain 50 §:n 4 momentin kumoamisesta</docTitle>
            </p>
        </preface>
        <preamble>
            <formula name="enactingClause">
