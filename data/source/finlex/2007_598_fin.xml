        <preface>
            <p>
                <docNumber>598/2007</docNumber>
                <docTitle>Laki valtiontilintarkastajain eräistä tarkastus- ja tiedonsaantioikeuksista annetun lain kumoamisesta</docTitle>
            </p>
        </preface>
        <preamble>
            <formula name="enactingClause">
