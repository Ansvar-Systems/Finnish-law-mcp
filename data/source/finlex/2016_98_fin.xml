        <preface>
            <p>
                <docNumber>98/2016</docNumber>
                <docTitle>Laki eläkesäätiölain 10 §:n muuttamisesta</docTitle>
            </p>
        </preface>
        <preamble>
            <formula name="enactingClause">
                <p>Eduskunnan päätöksen mukaisesti</p>
                <blockContainer>
                    <block name="substitutions">
                        <i>muutetaan </i>
                        eläkesäätiölain (
                        <affectedDocument href="/akn/fi/act/statute/1995/1774">1774/1995</affectedDocument>
                        ) 10 § seuraavasti:
                    </block>
