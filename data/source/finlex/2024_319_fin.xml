        <preface>
            <p>
                <docNumber>319/2024</docNumber>
                <docTitle>Laki tieliikennettä koskevaan yleissopimukseen tehdyistä muutoksista </docTitle>
            </p>
        </preface>
        <preamble>
            <formula name="enactingClause">
