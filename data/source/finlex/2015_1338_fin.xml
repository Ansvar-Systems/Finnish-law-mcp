        <preface>
            <p>
                <docNumber>1338/2015</docNumber>
                <docTitle>Laki kasvinjalostustoiminnan edistämisestä annetun lain kumoamisesta</docTitle>
            </p>
        </preface>
        <preamble>
            <formula name="enactingClause">
                <p>Eduskunnan päätöksen mukaisesti säädetään:</p>
            </formula>
