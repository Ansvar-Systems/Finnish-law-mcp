        <preface>
            <p>
                <docNumber>987/2015</docNumber>
                <docTitle>Lag om ändring av lagen om säker lastning och lossning av vissa bulkfartyg</docTitle>
            </p>
        </preface>
        <preamble>
            <formula name="enactingClause">
                <p>I enlighet med riksdagens beslut</p>
                <blockContainer>
