        <preface>
            <p>
                <docNumber>524/2015</docNumber>
                <docTitle>Lag om ändring av 4 § i narkotikalagen</docTitle>
            </p>
        </preface>
        <preamble>
            <formula name="enactingClause">
                <p>I enlighet med riksdagens beslut</p>
                <blockContainer>
                    <block name="substitutions">
                        <i>ändras</i>
                         i narkotikalagen (
                        <affectedDocument href="/akn/fi/act/statute/2008/373">373/2008</affectedDocument>
                        ) 4 § som följer:
                    </block>
                </blockContainer>
            </formula>
