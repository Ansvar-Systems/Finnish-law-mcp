        <preface>
            <p>
                <docNumber>257/2024</docNumber>
                <docTitle>Laki väkivallan ja häirinnän poistamisesta työelämässä tehdystä Kansainvälisen työjärjestön yleissopimuksesta</docTitle>
            </p>
        </preface>
        <preamble>
            <formula name="enactingClause">
                <p>Eduskunnan päätöksen mukaisesti säädetään:</p>
            </formula>
