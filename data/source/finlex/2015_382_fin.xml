        <preface>
            <p>
                <docNumber>382/2015</docNumber>
                <docTitle>Laki oikeudesta yleisiin vesialueisiin annetun lain 4 §:n muuttamisesta</docTitle>
            </p>
        </preface>
        <preamble>
            <formula name="enactingClause">
