        <preface>
            <p>
                <docNumber>299/2006</docNumber>
                <docTitle>Laki eläinten lääkitsemisestä annetun lain muuttamisesta</docTitle>
            </p>
        </preface>
        <preamble>
            <formula name="enactingClause">
                <p>Eduskunnan päätöksen mukaisesti</p>
                <blockContainer>
                    <block name="substitutions">
                        <i>muutetaan</i>
                         eläinten lääkitsemisestä 27 päivänä kesäkuuta 1997 annetun lain (
                        <affectedDocument href="/akn/fi/act/statute/1997/617">617/1997</affectedDocument>
                        ) 4 §:n 7 ja 9 kohta sekä 5 ja 6 § sekä
                    </block>
                </blockContainer>
                <blockContainer>
                    <block name="insertions">
                        <i>lisätään</i>
                         lakiin uusi 5 a § seuraavasti:
                    </block>
                </blockContainer>
            </formula>
