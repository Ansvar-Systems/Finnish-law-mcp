        <preface>
            <p>
                <docNumber>611/2005</docNumber>
                <docTitle>Laki ajoneuvoverolain muuttamisesta</docTitle>
            </p>
        </preface>
        <preamble>
            <formula name="enactingClause">
                <p>Eduskunnan päätöksen mukaisesti</p>
                <blockContainer>
                    <block name="repeals">
                        <i>kumotaan</i>
                         30 päivänä joulukuuta 2003 annetun ajoneuvoverolain (
                        <affectedDocument href="/akn/fi/act/statute/2003/1281">1281/2003</affectedDocument>
                        ) 64 §,
                    </block>
                </blockContainer>
                <blockContainer>
                    <block name="substitutions">
                        <i>muutetaan</i>
                         52 §:n 1 momentti ja 62 §:n 1 momentti sekä
                    </block>
                </blockContainer>
                <blockContainer>
                    <block name="insertions">
                        <i>lisätään</i>
                         62 §:ään uusi 3 momentti seuraavasti:
                    </block>
                </blockContainer>
            </formula>
