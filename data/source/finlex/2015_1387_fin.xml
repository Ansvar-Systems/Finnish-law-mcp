        <preface>
            <p>
                <docNumber>1387/2015</docNumber>
                <docTitle>Laki ajoneuvojen katsastustoiminnasta annetun lain muuttamisesta</docTitle>
            </p>
        </preface>
        <preamble>
            <formula name="enactingClause">
                <p>Eduskunnan päätöksen mukaisesti</p>
