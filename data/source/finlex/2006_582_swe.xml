        <preface>
            <p>
                <docNumber>582/2006</docNumber>
                <docTitle>Lag om ändring av sjömanslagen</docTitle>
            </p>
        </preface>
        <preamble>
            <formula name="enactingClause">
                <p>I enlighet med riksdagens beslut</p>
                <blockContainer>
                    <block name="insertions">
                        <i>fogas</i>
                         till sjömanslagen av den 7 juni 1978 (
                        <affectedDocument href="/akn/fi/act/statute/1978/423">423/1978</affectedDocument>
                        ) en ny 32 i § som följer:
                    </block>
