        <preface>
            <p>
                <docNumber>660/2024</docNumber>
                <docTitle>Laki elintarvikelain 5 §:n muuttamisesta </docTitle>
            </p>
        </preface>
        <preamble>
            <formula name="enactingClause">
                <p>Eduskunnan päätöksen mukaisesti </p>
                <blockContainer>
                    <block name="substitutions">
                        <i>muutetaan</i>
                         elintarvikelain (
                        <affectedDocument href="/akn/fi/act/statute/2021/297">297/2021</affectedDocument>
                        ) 5 §:n 2 momentin 32 kohta seuraavasti:
                    </block>
                </blockContainer>
