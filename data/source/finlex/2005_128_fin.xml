        <preface>
            <p>
                <docNumber>128/2005</docNumber>
                <docTitle>Laki säädettyjen määräaikain laskemisesta annetun lain 6 §:n muuttamisesta</docTitle>
            </p>
        </preface>
        <preamble>
            <formula name="enactingClause">
