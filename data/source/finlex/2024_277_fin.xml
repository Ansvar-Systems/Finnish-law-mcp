        <preface>
            <p>
                <docNumber>277/2024</docNumber>
                <docTitle>Laki työttömyysetuuksien rahoituksesta annetun lain muuttamisesta annetun lain muuttamisesta </docTitle>
            </p>
        </preface>
        <preamble>
            <formula name="enactingClause">
