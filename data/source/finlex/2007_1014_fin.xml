        <preface>
            <p>
                <docNumber>1014/2007</docNumber>
                <docTitle>Laki holhoustoimen edunvalvontapalveluiden järjestämisestä annetun lain 6 §:n muuttamisesta</docTitle>
            </p>
        </preface>
        <preamble>
            <formula name="enactingClause">
