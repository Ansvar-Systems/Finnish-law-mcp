        <preface>
            <p>
                <docNumber>1148/2005</docNumber>
                <docTitle>Lag om ändring av 1 och 4 § i lagen om undanröjande av internationell dubbelbeskattning</docTitle>
            </p>
        </preface>
        <preamble>
            <formula name="enactingClause">
