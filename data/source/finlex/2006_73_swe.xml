        <preface>
            <p>
                <docNumber>73/2006</docNumber>
                <docTitle>Lag om ändring av 1 § i lagen om finansiering av undervisnings- och kulturverksamhet</docTitle>
            </p>
        </preface>
        <preamble>
            <formula name="enactingClause">
