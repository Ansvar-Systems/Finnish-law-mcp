        <preface>
            <p>
                <docNumber>446/2024</docNumber>
                <docTitle>Laki yrityspalvelujen asiakastietojärjestelmästä annetun lain 10 §:n muuttamisesta</docTitle>
            </p>
        </preface>
        <preamble>
            <formula name="enactingClause">
