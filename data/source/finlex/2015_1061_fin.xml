        <preface>
            <p>
                <docNumber>1061/2015</docNumber>
                <docTitle>Laki öljyvahinkojen torjuntalain 29 §:n muuttamisesta</docTitle>
            </p>
        </preface>
        <preamble>
            <formula name="enactingClause">
                <p>Eduskunnan päätöksen mukaisesti</p>
                <blockContainer>
                    <block name="substitutions">
                        <i>muutetaan</i>
                         öljyvahinkojen torjuntalain (
                        <affectedDocument href="/akn/fi/act/statute/2009/1673">1673/2009</affectedDocument>
                        ) 29 § seuraavasti:
                    </block>
                </blockContainer>
            </formula>
